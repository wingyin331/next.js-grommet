  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-imports": "^1.5.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-parallel": "^0.3.2",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
